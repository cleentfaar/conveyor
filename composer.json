{
    "name": "webcreate/conveyor",
    "type": "library",
    "keywords": ["build", "deploy", "php", "tool", "commandline"],
    "description": "Build and deploy tool for PHP",
    "homepage": "http://conveyordeploy.com/",
    "license": "Dual-license",
    "authors": [
        {
            "name": "Jeroen Fiege",
            "email": "jeroen@webcreate.nl",
            "homepage": "http://webcreate.nl"
        }
    ],
    "autoload": {
        "psr-0": {
            "Webcreate": ["src/", "tests/"]
        }
    },
    "require": {
        "php": ">=5.4",
        "ext-ftp": "*",
        "symfony/console": "2.2.*",
        "symfony/config": "2.2.*",
        "symfony/yaml": "2.2.*",
        "symfony/finder": "2.2.*",
        "symfony/filesystem": "2.2.*",
        "symfony/event-dispatcher": "2.2.*",
        "symfony/dependency-injection": "2.2.*",
        "phpseclib/phpseclib": "*",
        "webcreate/vcs": "~1.2",
        "webcreate/util": "1.*",
        "monolog/monolog": "1.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "bin": ["bin/conveyor"]
}
